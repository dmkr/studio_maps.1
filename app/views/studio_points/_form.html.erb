<%= simple_form_for(@studio_point) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :lat %>
    <%= f.input :lon %>
    <%= f.input :studio_id %>
    <%= f.input :street %>
    <%= f.input :city %>
    <%= f.input :zip %>
    <%= f.input :state %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
