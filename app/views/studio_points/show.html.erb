<%- model_class = StudioPoint -%>
<div class="page-header">
  <h1><%=t '.title', :default => StudioPoint %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:lat) %></dt>
      <dd><pre class="prettyprint"><%= @studio_point.lat %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:lon) %></dt>
      <dd><pre class="prettyprint"><%= @studio_point.lon %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:studio_id) %></dt>
      <dd><pre class="prettyprint"><%= @studio_point.studio_id %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:street) %></dt>
      <dd><pre class="prettyprint"><%= @studio_point.street %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:city) %></dt>
      <dd><pre class="prettyprint"><%= @studio_point.city %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:zip) %></dt>
      <dd><pre class="prettyprint"><%= @studio_point.zip %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:state) %></dt>
      <dd><pre class="prettyprint"><%= @studio_point.state %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              studio_points_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_studio_point_path(@studio_point), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              studio_point_path(@studio_point),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

